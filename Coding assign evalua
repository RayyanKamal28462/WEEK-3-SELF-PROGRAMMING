package practice;

public class assignmentevaluation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum1 = 100 + 50;        
		int sum2 = sum1 + 250;      
		int sum3 = sum2 + sum2; 
		
		System.out.println("sum1 is:" + sum1);
		System.out.println("sum2 is:" + sum2);
		System.out.println("sum3 is:" + sum3);

	}

}
