package practice;

public class baseonweek3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int oneThousand =1000;
int oneMillion = 1000 * oneThousand;
int oneBillion = 1000 * oneMillion;
int threeBillion = 3 * oneBillion;
System.out.println(threeBillion);


	}

}

public class bow3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "Java Exercises!";
        System.out.println("Original String = " + str);
        
        int index1 = str.charAt(0);
        int index2 = str.charAt(10);

      
        System.out.println("The character at  0 is " +
            (char)index1);
        System.out.println("The character at 10 is " +
            (char)index2);	
      
	}
	

}

