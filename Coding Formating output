package practice;

public class formatingoutput {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.printf("%.2f", 1.22);
		
		System.out.printf("%10.2f", 1.22);
	
		System.out.println("Value of x is %5d and value of y is %6.2f  %n‚Äù, x , y");
System.out.printf("%-10s" , "Total:");

System.out.printf("%10.2f" , 17.29);

	System.out.printf("%-10s%10.2f" , "Total" , 17.29);
	}

}
